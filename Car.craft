    - redstone_block
    - redstone_lamp
    - redstone_torch
    - redstone_wall_torch
    - redstone_wire
    - repeater
    - sticky_piston
    - target
    - tripwire
    - tripwire_hook
    # DECORATIVE BLOCKS
    - "#banners"
    - "#carpets"
    - "#heads"
    - "#signs"
    - beacon
    - bell
    - bookshelf
    - cake
    - chiseled_bookshelf
    - dragon_egg
    - fire
    - flower_pot
    - glowstone
    - jukebox
    - lantern
    - torch
    - wall_torch
    # UTILITY BLOCKS
    - "#anvil"
    - "#beds"
    - "#chests"
    - "#doors"
    - "#fence_gates"
    - "#fences"
    - "#trapdoors"
    - "#walls"
    - blast_furnace
    - brewing_stand
    - cartography_table
    - cauldron
    - fletching_table
    - composter
    - grindstone
    - lectern
    - loom
    - smithing_table
    - smoker
    - stonecutter
    - crafting_table
    - enchanting_table
    - ender_chest
    - furnace
    - ladder
    - lightning_rod
    - slime_block
    - lava
    - water
    # FORBIDDEN BLOCKS
    - bedrock 

forbiddenBlocks:
    - bedrock

onlyMovePlayers: false
moveEntities: true
canDirectControl: false

# How the craft moves
canFly: false
allowVerticalMovement: false
canHover: false
canHoverOverWater: false
useGravity: true
hoverLimit: 1
speed: 3.0
canCruise: true
cruiseSpeed: 4.0
cruiseSkipBlocks: 1
fuelBurnRate: 0.04
rotateAtMidpoint: false

# Craft detection. Distance^2 < 10 * (craft size) * (active multiplier)
detectionMultiplier: 4.0
underwaterDetectionMultiplier: 4.0

# How the craft acts when it crashes or sinks
collisionExplosion: 0.0
sinkPercent: 99.0
overallSinkPercent: 99.0 # Cars and tanks are harder to sink because they only need redstone, so give them less margine
keepMovingOnSink: true
tryNudge: false

flyblocks:
    redstone_block:
        - 10.0
        - 100.0
    dispenser:
        - 0.0
        - N2
    "#anvil": # anvils are OP armor, so limit it
        - 0.0
        - 1.0
    water: # so is water
        - 0.0
        - 1.0
    coal_block: # Hard limit for all craft types on maximum torpedoes
        - 0.0
        - 5.0

passthroughBlocks:
    - grass
    - tall_grass
    - fern
    - large_fern
    - "#flowers"
    - dead_bush
    - small_dripleaf
    - big_dripleaf

