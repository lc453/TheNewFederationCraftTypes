minHeightLimit: 50
maxHeightLimit: 80
requireWaterContact: true
allowVerticalMovement: false
speed: 1.5
cruiseSpeed: 3.0
tryNudge: false
flyblocks:
    [oak_planks, acacia_planks, dark_oak_planks, bamboo_planks, jungle_planks, birch_planks, cherry_planks, mangrove_planks, spruce_planks]:
        - 25.0
        - 100.0
    "#anvil": # anvils are OP armor, so limit it
        - 0.0
        - 1.0
    obsidian: # so is obsidian
        - 0.0
        - 1.0
    water: # so is water
        - 0.0
        - 1.0
    ["#terracotta", iron_block]: #limit armor for frigates because they don't use fuel
        - 0.0
        - 25.0


passthroughBlocks:
    - kelp_plant
    - seagrass
    - kelp
    - tall_seagrass
    - bubble_column
allowCannonDirectors: true
allowAADirectors: true